{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "baseBranches": [
    "develop"
  ],
  "packageRules": [
    {
      "description": "Example dependencies",
      "matchFileNames": ["examples/vite/package.json"],
      "groupName": "Example's package.json changes"
    },
    {
      "matchFileNames": [
        "**/package.json"
      ],
      "matchUpdateTypes": [
        "major",
        "minor"
      ]
    }
  ]
}
