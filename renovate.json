{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "baseBranches": [
    "develop"
  ],
  "packageRules": [
    {
      "matchPaths": [
        "examples/vites/**"
      ],
      "rangeStrategy": "bump"
    },
    {
      "matchPackagePatterns": [
        "*"
      ],
      "rangeStrategy": "bump"
    }
  ]
}
